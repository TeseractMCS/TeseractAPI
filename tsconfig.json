{
    "compilerOptions": {
        "allowJs": true,
        "module": "es2020",
        "moduleResolution": "node",
        "lib": ["es2020", "DOM"],
        "strict": false,
        "target": "es2020",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,

        "noEmitOnError": true,
        "strictBindCallApply": false,
        "useUnknownInCatchVariables": false,
        "noImplicitOverride": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "allowUnusedLabels": true,
        "skipLibCheck": true,
        "alwaysStrict": true,
        "preserveConstEnums": true,
        "newLine": "lf",
        "types": [],
        "declaration": true,
        "noLib": false,
        "sourceMap": true,
        "pretty": true,
        "forceConsistentCasingInFileNames": true,
        "allowUnreachableCode": true,
        "noImplicitReturns": false,
        "noImplicitUseStrict": false,
        "outDir": "build/",
        "rootDir": ".",
        "listFiles": false,
        "noEmitHelpers": true,
        "baseUrl": "./src",
        "paths": {
            "TeseractAPI/*": ["./TeseractAPI/*"]
        }
    },
    // "include": ["scripts/**/*", "scripts/**/*.js"],
    "exclude": ["node_modules", "exports", "old_version", "behavior_src", "scripts"],
    "compileOnSave": false
}
